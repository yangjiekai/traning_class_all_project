<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.project.ultra.jack.GPSlocation">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
        android:id="@+id/textView_GPS2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textView_GPS1"
        android:layout_below="@+id/textView_GPS2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:id="@+id/button42"
        android:layout_below="@+id/textView_GPS1"
        android:layout_alignRight="@+id/textView_GPS2"
        android:layout_alignEnd="@+id/textView_GPS2"
        android:layout_marginTop="155dp"
        android:onClick="click_GPS_1" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2"
        android:id="@+id/button43"
        android:layout_below="@+id/button42"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="click_GPS_2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textView_GPS_8_9"
        android:layout_alignBaseline="@+id/button43"
        android:layout_alignBottom="@+id/button43"
        android:layout_toRightOf="@+id/button43"
        android:layout_toEndOf="@+id/button43"
        android:layout_marginLeft="45dp"
        android:layout_marginStart="45dp" />
</RelativeLayout>
