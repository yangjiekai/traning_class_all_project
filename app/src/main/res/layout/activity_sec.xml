<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.project.ultra.jack.SecActivity"
    android:onClick="click11">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="回主頁"
        android:id="@+id/button5"
        android:onClick="click4"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="51dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textView5"
        android:textSize="16dp"
        android:layout_below="@+id/button5"
        android:layout_alignParentLeft="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="WebView"
        android:id="@+id/button7"
        android:onClick="click7"
        android:layout_alignTop="@+id/button12"
        android:layout_toEndOf="@+id/button12" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="可以指定連到另一個 APP的某頁面"
        android:id="@+id/button11"
        android:onClick="click11"
        android:layout_above="@+id/textView5"
        android:layout_alignStart="@+id/button7" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="連到網頁"
        android:id="@+id/button12"
        android:layout_alignParentStart="true"
        android:onClick="click12"
        android:layout_below="@+id/button11"
        android:layout_alignParentLeft="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="打電話"
        android:id="@+id/button13"
        android:onClick="click13"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button12"
        android:layout_alignParentLeft="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="連地圖(經緯)"
        android:id="@+id/button14"
        android:onClick="click14"
        android:layout_below="@+id/button13"
        android:layout_alignParentLeft="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="連地圖(地址)"
        android:id="@+id/button15"
        android:onClick="click15"
        android:textSize="36sp"
        android:fontFamily="sans-serif-light"
        android:textColor="@android:color/black"
        android:background="#ccddff"
        android:padding="20dp"
        android:layout_alignTop="@+id/button14"
        android:layout_toEndOf="@+id/button14" />


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="分享文字"
        android:id="@+id/button16"
        android:onClick="click16"
        android:layout_below="@+id/button15"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Intent"
        android:id="@+id/textView8"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:textColor="#c51717"
        android:textSize="20sp" />
</RelativeLayout>
